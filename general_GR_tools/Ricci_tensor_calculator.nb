 RowBox[{
  RowBox[{
   RowBox[{"christoffel", "[", 
    RowBox[{"coords_", ",", "metric_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"dim", "=", 
        RowBox[{"dim", "[", "coords", "]"}]}], ",", 
       RowBox[{"metricinv", "=", 
        RowBox[{"metricinv", "[", "metric", "]"}]}]}], "}"}], ",", 
     RowBox[{"Simplify", "[", 
      RowBox[{
       FractionBox["1", "2"], "*", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"metricinv", "[", 
              RowBox[{"[", "\[Mu]", "]"}], "]"}], "[", 
             RowBox[{"[", "\[Sigma]", "]"}], "]"}], 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"metric", "[", 
                  RowBox[{"[", "\[Sigma]", "]"}], "]"}], "[", 
                 RowBox[{"[", "\[Nu]", "]"}], "]"}], ",", 
                RowBox[{"coords", "[", 
                 RowBox[{"[", "\[Rho]", "]"}], "]"}]}], "]"}], "+", 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"metric", "[", 
                  RowBox[{"[", "\[Sigma]", "]"}], "]"}], "[", 
                 RowBox[{"[", "\[Rho]", "]"}], "]"}], ",", 
                RowBox[{"coords", "[", 
                 RowBox[{"[", "\[Nu]", "]"}], "]"}]}], "]"}], "-", 
              RowBox[{"D", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"metric", "[", 
                  RowBox[{"[", "\[Nu]", "]"}], "]"}], "[", 
                 RowBox[{"[", "\[Rho]", "]"}], "]"}], ",", 
                RowBox[{"coords", "[", 
                 RowBox[{"[", "\[Sigma]", "]"}], "]"}]}], "]"}]}], ")"}]}], 
           ",", 
           RowBox[{"{", 
            RowBox[{"\[Sigma]", ",", "dim"}], "}"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Mu]", ",", "dim"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Nu]", ",", "dim"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Rho]", ",", "dim"}], "}"}]}], "]"}]}], "]"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{"christoffel", "[", 
  RowBox[{"coords", ",", "metric"}], "]"}]}], "Code",
