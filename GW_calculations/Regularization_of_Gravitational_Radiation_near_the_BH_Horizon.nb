Cell[BoxData[
 RowBox[{"(*", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Tau]", "[", "a_", "]"}], " ", ":=", " ", 
    RowBox[{
     FractionBox["I", 
      RowBox[{"Sqrt", "[", "2", "]"}]], "*", "a", "*", 
