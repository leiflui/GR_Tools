       RowBox[{"\[Tau]", "[", "a", "]"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"M", "\[Element]", "Reals"}], ",", 
        RowBox[{"a", "\[Element]", "Reals"}], ",", 
        RowBox[{"r", "\[Element]", "Reals"}], ",", 
        RowBox[{"\[Theta]", "\[Element]", "Reals"}]}], "}"}]}], "]"}]}], ";", 
   "\n", 
   RowBox[{
    RowBox[{"\[Alpha]", "[", "a_", "]"}], " ", ":=", " ", 
    RowBox[{
     FractionBox[
      RowBox[{"\[Rho]", "[", "a", "]"}], 
      RowBox[{"2", "*", 
       RowBox[{"Sqrt", "[", "2", "]"}]}]], "*", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Cot", "[", "\[Theta]", "]"}], "+", 
       RowBox[{"2", "*", "I", "*", "a", "*", 
        RowBox[{"\[Rho]", "[", "a", "]"}]}]}], ")"}]}]}], ";", "\n", 
   RowBox[{
    RowBox[{"\[Beta]", "[", "a_", "]"}], " ", ":=", " ", 
    RowBox[{
     FractionBox[
      RowBox[{"\[Rho]bar", "[", "a", "]"}], 
      RowBox[{"2", "*", 
       RowBox[{"Sqrt", "[", "2", "]"}]}]], "*", 
     RowBox[{"Cot", "[", "\[Theta]", "]"}]}]}], ";", "\n", 
   RowBox[{
    RowBox[{"\[Beta]bar", "[", "a_", "]"}], " ", ":=", " ", 
    RowBox[{"Refine", "[", 
     RowBox[{
      RowBox[{"Conjugate", "[", 
       RowBox[{"\[Beta]", "[", "a", "]"}], "]"}], ",", 
      RowBox[{"{", 
